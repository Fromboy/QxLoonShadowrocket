#!name=Google CN Rewrite
#!desc=将 google.cn 重定向到 google.com
#!URL=https://raw.githubusercontent.com/Fromboy/QxLoonShadowrocket/refs/heads/Loon/GoogleCNRewrite.plugin

[Rewrite]
^https?:\/\/(www\.)?(g|google)\.cn https://www.google.com 307
^https?:\/\/(www\.)?google\.cn\/search https://www.google.com/search 307

[MITM]
hostname = g.cn,www.g.cn,google.cn,www.google.cn
