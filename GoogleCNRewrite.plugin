[Rewrite]
^https?:\/\/(www\.)?(g|google)\.cn https://www.google.com 307
^https?:\/\/(www\.)?google\.cn\/search https://www.google.com/search 307

[MITM]
hostname = g.cn,www.g.cn,google.cn,www.google.cn
